{
  "name": "jintrick-coding-extension",
  "version": "1.5.0",
  "description": "Coding assistance extensions including linters, skill management, and automated code review.",
  "contextFileName": "GEMINI.md",
  "hooks": [
    {
      "event": "on_tool_call",
      "path": "dist/hooks/linter_hook.cjs"
    },
    {
      "event": "on_tool_call",
      "path": "dist/hooks/open_file_hook.cjs"
    },
    {
      "event": "on_tool_call",
      "path": "dist/hooks/expert_docs_hook.cjs"
    }
  ],
  "agents": [
    {
      "path": "agents/code-reviewer.md"
    }
  ],
  "commands": [
    {
      "path": "commands/review.toml"
    }
  ]
}
